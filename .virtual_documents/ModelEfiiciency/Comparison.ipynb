    'BlendProperty3': 2.45,
    'BlendProperty4': 26.26,
    'BlendProperty5': 0.17,
    'BlendProperty6': 11.01,
    'BlendProperty7': 1.98,
    'BlendProperty8': 1.80,
    'BlendProperty9': 18.43,
    'BlendProperty10': 0.60
}

lgbm_mape = {
    'BlendProperty1': 0.9187,
    'BlendProperty2': 1.4066,
    'BlendProperty3': 2.3454,
    'BlendProperty4': 19.2350,
    'BlendProperty5': 0.8199,
